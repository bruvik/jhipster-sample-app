pipeline {
  agent any
  stages {
    stage('Build Backend') {
      steps {
        echo 'Build backend'
      }
    }
    stage('Test Backend') {
      steps {
        echo 'test backend'
      }
    }
    stage('More Tests') {
      steps {
        echo 'more tests'
      }
    }
    stage('Deploy Staging') {
      steps {
        echo 'deploying  staging'
      }
    }
    stage('Deploy Test') {
      steps {
        echo 'deploy test'
      }
    }
  }
}